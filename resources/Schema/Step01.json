		"input": [
			{
				"id": "transportationStep.nameField",
				"category": "field",
				"conditions": {
					"required": [
						{
							"expression": "true",
							"error": "Transportation name is required"
						}
					],
					"validity": [
						{
							"expression": "isEmpty(name?.value) || name.value.length >= 3",
							"error": "Name must be at least 3 characters"
						},
						{
							"expression": "isEmpty(name?.value) || name.value.length <= 255",
							"error": "Name must not exceed 255 characters"
						}
					]
				},
				"margin": {
					"top": 24
				},
				"flexProperties": {
					"grow": 1
				},
				"code": "name",
				"label": {
					"text": "Transportation Name"
				},
				"placeholder": "Enter transportation name",
				"maxLength": 255
			}
		],
		"dateTime": [
			{
				"id": "transportationStep.pickupFromField",
				"category": "field",
				"conditions": {
					"required": [
						{
							"expression": "true",
							"error": "Pickup start date is required"
						}
					],
					"validity": [
						{
							"expression": "isEmpty(pickupFrom?.value) || pickupFrom.value >= new Date()",
							"error": "Pickup start date must be in the future"
						},
						{
							"expression": "isEmpty(pickupFrom?.value) || isEmpty(pickupTo?.value) || pickupFrom.value <= pickupTo.value",
							"error": "Pickup start date must be before or equal to end date"
						}
					]
				},
				"margin": {
					"top": 24
				},
				"flexProperties": {
					"basis": "300px"
				},
				"code": "pickupFrom",
				"label": {
					"text": "Pickup From"
				},
				"placeholder": "Select start date and time",
				"showTime": true,
				"minDate": "today"
			},
			{
				"id": "transportationStep.pickupToField",
				"category": "field",
				"conditions": {
					"required": [
						{
							"expression": "true",
							"error": "Pickup end date is required"
						}
					],
					"validity": [
						{
							"expression": "isEmpty(pickupTo?.value) || pickupTo.value >= new Date()",
							"error": "Pickup end date must be in the future"
						},
						{
							"expression": "isEmpty(pickupFrom?.value) || isEmpty(pickupTo?.value) || pickupFrom.value <= pickupTo.value",
							"error": "Pickup end date must be after or equal to start date"
						}
					]
				},
				"margin": {
					"top": 24
				},
				"flexProperties": {
					"basis": "300px"
				},
				"code": "pickupTo",
				"label": {
					"text": "Pickup To"
				},
				"placeholder": "Select end date and time",
				"showTime": true,
				"minDate": "pickupFrom"
			}
		],
